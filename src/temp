const renderShapes = [];

for(var i=0;i<2;i++)
{
    for(var j=0;j<2;j++)
    {
        const randomShape = geometries.children[j];
        randomShape.position.set(i,j,2);
        // console.log(randomShape)
        renderShapes.push({shape:randomShape,position:new THREE.Vector3(i,j,2)});
        // scene.add(randomShape);
    }
}

for(var i=0;i<renderShapes.length;i++)
{
    const temp = renderShapes[i].shape;
    temp.position.set(renderShapes[i].position);

    console.log(temp);
    scene.add(temp);
}
